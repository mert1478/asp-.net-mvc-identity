@using IdentityApp.Models
@model AspNetCustomers
@{
    ViewData["Title"] = "UpdateCustomer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Customer Update Page</h1>

<form method="post">
    <div class="form-group">
        <label>Name</label>
        <input type="text" name="Name" value="@Model.Name" class="form-control" />
    </div>
    <div class="form-group">
        <label>Explanation</label>
        <input type="text" name="Explanation" value="@Model.Explanation" class="form-control" />
    </div>
    <div class="form-group">
        <label>Data</label>
        <input type="text" name="Data" value="@Model.Data" class="form-control" />
    </div>

    <button class="btn btn-success">Get Customer Update </button>
</form>
